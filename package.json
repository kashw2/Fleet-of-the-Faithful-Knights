{
  "name": "fleet-of-the-faithful-knights",
  "version": "1.0.0",
  "description": "FFK Monorepo",
  "scripts": {
    "build": "echo \"$(tput setaf 2)Running Build Script$(tput srg0)\"",
    "build:dev": "echo \"$(tput setaf 2)Running Development Build Script$(tput srg0)\"",
    "build-libs": "echo \"$(tput setaf 2)Running Library Build Script$(tput srg0)\" && npm run build-util && npm run build-external && npm run build-ts && npm run build-server",
    "build-util": "lerna run build --scope=@kashw2/lib-util",
    "build-external": "lerna run build --scope=@kashw2/lib-external",
    "build-server": "lerna run build --scope=@kashw2/lib-server",
    "build-ts": "lerna run build --scope=@kashw2/lib-ts"
  },
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kashw2/Fleet-of-the-Faithful-Knights.git"
  },
  "author": "Keanu Ashwell",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/kashw2/Fleet-of-the-Faithful-Knights/issues"
  },
  "homepage": "https://github.com/kashw2/Fleet-of-the-Faithful-Knights#readme",
  "dependencies": {
    "@kashw2/lib-external": "4.0.1",
    "@kashw2/lib-server": "4.0.0",
    "@kashw2/lib-ts": "4.0.1",
    "@kashw2/lib-util": "3.0.0",
    "@types/cors": "2.8.12",
    "@types/express": "4.17.13",
    "@types/mssql": "7.1.5",
    "@types/node": "16.11.26",
    "@types/passport": "1.0.7",
    "axios": "0.26.1",
    "body-parser": "1.20.0",
    "cors": "2.8.5",
    "express": "4.17.3",
    "funfix-core": "7.0.1",
    "immutable": "4.0.0",
    "lerna": "4.0.0",
    "moment": "2.29.2",
    "mssql": "8.1.0",
    "rxjs": "7.5.5",
    "screenfull": "5.2.0",
    "ts-mixer": "6.0.1",
    "tslib": "2.3.1",
    "typescript": "4.6.3"
  },
  "devDependencies": {
    "assert-browserify": "2.0.0",
    "browserify-zlib": "0.2.0",
    "os-browserify": "0.3.0",
    "querystring-browser": "1.0.4",
    "stream-browserify": "3.0.0",
    "ts-node": "10.7.0",
    "tty-browserify": "0.0.1",
    "url-browser": "0.1.7",
    "util-browser": "0.0.2",
    "gh-pages": "3.2.3",
    "typedoc": "0.22.15",
    "open-cli": "7.0.1",
    "run-p": "0.0.0",
    "npm-run-all": "4.1.5",
    "tslint": "6.1.3"
  },
  "workspaces": [
    "./",
    "./api",
    "./onboarding",
    "./panel",
    "./libs/angular",
    "./libs/external",
    "./libs/server",
    "./libs/typescript",
    "./libs/util"
  ]
}
